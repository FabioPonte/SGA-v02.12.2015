        <Button x:Name="btn_tudo" Content="Marcar Tudo" HorizontalAlignment="Right" Margin="0,576,288,0" VerticalAlignment="Top" Width="109" Height="31" Click="btn_tudo_Click"/>
        <Button x:Name="btn_nada" Content="Desmarcar Tudo" HorizontalAlignment="Right" Margin="0,576,174,0" VerticalAlignment="Top" Width="109" Height="31" Click="btn_nada_Click"/>
        <Button x:Name="btn_gravar" Content="Gravar" HorizontalAlignment="Right" Margin="0,0,22,25" VerticalAlignment="Bottom" Width="187" Height="82" FontSize="26.667" Click="btn_gravar_Click"/>

    </Grid>
